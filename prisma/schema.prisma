// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider   = "dart run lib/dev.dart"
  // output     = "."
  outputType = ["a"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./prisma.sqlite"
}

model User {
  id        String   @id @map("_id")
  name      String   @unique
  createdAt DateTime @default(now())
}
